{
  "name": "@muselesscreator/use-keyed-state",
  "version": "0.0.0",
  "description": "React toolkit for writing and testing react components",
  "main": "dist/index.js",
  "release": {
    "branches": [
      "main",
      "next"
    ]
  },
  "files": [
    "/dist",
    "/src"
  ],
  "exports": {
    ".": {
      "import": "/dist/esm/index.js",
      "default": "dist/index.js"
    }
  },
  "author": "muselesscreator",
  "license": "AGPL-3.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muselesscreator/use-keyed-state.git"
  },
  "homepage": "https://github.com/muselesscreator/use-keyed-state#README",
  "scripts": {
    "dev": "vite",
    "build": "tsc && tsup src/index.ts --format cjs,esm --minify --dts --legacy-output dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "release": "release-it",
    "tsc": "tsc",
    "test": "vitest --coverage"
  },
  "devDependencies": {
    "@muselesscreator/get-effects": "^0.0.1",
    "@muselesscreator/strict-dict": "^0.0.3",
    "@reduxjs/toolkit": "^1.5.1",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "14.2.1",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-is": "^18.2.4",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^1.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.0.0",
    "lodash": "^4.17.21",
    "pretty-format": "^29.7.0",
    "react": "^18.2.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.2.0",
    "react-intl": "^6.6.2",
    "react-test-renderer": "18.2.0",
    "release-it": "^17.0.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.2.2",
    "v8-to-istanbul": "^9.2.0",
    "vite": "^5.1.0",
    "vitest": "^1.2.2"
  }
}
